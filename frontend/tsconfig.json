{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "lib": ["dom", "es2019"],
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "typeRoots": ["./node_modules/@types", "./types"],
    "types": ["react", "react-dom", "node", "axios", "@mui/x-data-grid", "react-router-dom"]
  },
  "include": ["src"]
}


